<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Rollout_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( Rollout_From__c ), NOT( ISBLANK( Rollout_To__c )))</errorConditionFormula>
    <errorMessage>You must enter a Shipment start date</errorMessage>
</ValidationRule>
