<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cost_Center_Format_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK(Cost_Center__c)) &amp;&amp; LEN( Cost_Center__c) &lt;&gt; 16</errorConditionFormula>
    <errorDisplayField>Cost_Center__c</errorDisplayField>
    <errorMessage>Cost Center Field requireds 16 Characters in format: xxxx-xxxx-xxxxxx</errorMessage>
</ValidationRule>
