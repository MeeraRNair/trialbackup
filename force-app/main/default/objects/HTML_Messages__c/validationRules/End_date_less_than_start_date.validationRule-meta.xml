<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_date_less_than_start_date</fullName>
    <active>true</active>
    <errorConditionFormula>IF(  End_Date__c &lt; Start_Date__c , true, false)</errorConditionFormula>
    <errorDisplayField>CurrencyIsoCode</errorDisplayField>
    <errorMessage>The End Date must be on or after the Start Date</errorMessage>
</ValidationRule>
