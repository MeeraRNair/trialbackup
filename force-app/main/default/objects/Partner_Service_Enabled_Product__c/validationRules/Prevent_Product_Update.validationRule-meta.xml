<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Product_Update</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISCHANGED(Service_Enabled_Product__c),ISCHANGED(Service_Level__c))</errorConditionFormula>
    <errorMessage>You cannot change the Product or Service Level. Please delete this Product if it is no longer valid.</errorMessage>
</ValidationRule>
