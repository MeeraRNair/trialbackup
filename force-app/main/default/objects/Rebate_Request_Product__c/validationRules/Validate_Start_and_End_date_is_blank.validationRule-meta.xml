<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_Start_and_End_date_is_blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT(ISBLANK( End_Date__c )), ISBLANK(Start_Date__c)  )</errorConditionFormula>
    <errorMessage>Effective Date is required to enter Expiration Date</errorMessage>
</ValidationRule>
