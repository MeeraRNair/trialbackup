<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_on_Partner_Account</fullName>
    <active>false</active>
    <errorConditionFormula>AND( !(ISNEW()), 
ISCHANGED(Partner_Account__c)
)</errorConditionFormula>
