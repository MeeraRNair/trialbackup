<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Partner_Phone_Required</fullName>
    <active>true</active>
    <description>&quot;Partner Phone&quot; field is required if Channel Partner Involved? = Yes</description>
    <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Request_OEM_Tech_Partner_Support&apos;,ISBLANK(Partner_Phone__c), ISPICKVAL(Channel_Partner_Involved__c, &apos;Yes&apos;))</errorConditionFormula>
    <errorDisplayField>Partner_Phone__c</errorDisplayField>
    <errorMessage>You must enter a value</errorMessage>
</ValidationRule>
