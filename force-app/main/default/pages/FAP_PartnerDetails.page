            <apex:variable var="errorMsg" value="{!JSENCODE(HTMLENCODE($Label.msgaccessdenied))}" rendered="{!errorCode == aCCESSDENIED}"/>
            <apex:pageMessage severity="error" strength="2" title="{!errorMsg}"/>
