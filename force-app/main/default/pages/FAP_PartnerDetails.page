                                             <table>
                                                <tr>
                                                   <td>
                                                      <apex:image id="theImage" value="{!$Resource.greenbox_png}" rendered="{!OR(CONTAINS(msg,JSENCODE(HTMLENCODE($Label.msgpartnerunpublished))),CONTAINS(msg,JSENCODE(HTMLENCODE($Label.msgpartnerpublished))))}" width="20" height="20"/>
                                                      <apex:image id="theImage2" value="/img/msg_icons/error32.png" width="20" height="20" rendered="{!NOT(OR(CONTAINS(msg,JSENCODE(HTMLENCODE($Label.msgpartnerunpublished))),CONTAINS(msg,JSENCODE(HTMLENCODE($Label.msgpartnerpublished)))))}"/>
                                                   </td>
                                                   <td>
                                                      {!msg}
                                                   </td>
                                                </tr>
                                             </table>
                                          </li>
                                       </apex:repeat>
                                    </ul>
                                 </div>
                              </td>
                           </tr>
                        </table>
                     </div>
                  </td>
               </tr>
            </table>
         </apex:outputPanel>
