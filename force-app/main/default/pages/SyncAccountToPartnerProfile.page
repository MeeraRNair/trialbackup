<apex:page standardController="Account" extensions="VFCntrlr_SyncAccountToPartnerProfile" showheader="false" action="{!integratePartnerData}" standardStylesheets="true">
    <br/><br/><br/><br/>
    <table width="600px" cellpadding="0" cellspacing="0" align="center" border="0">
        <tr>
            <td>
                <apex:form >
                    <apex:outputPanel > 
                        <apex:pageMessage severity="confirm"
                            strength="2"
                            title="Successfully submitted"
                            detail="The synchronization request to Partner Profile database has been submitted successfully." />                                
                        
                        
                        <table align="center" >
                            <tr>
                                <td>
                                    <apex:commandButton value="OK" action="{!cancel}" />
                                </td>
                            </tr>
                        </table>
                    </apex:outputPanel>
                </apex:form>
            </td>
        </tr>
    </table>
</apex:page>