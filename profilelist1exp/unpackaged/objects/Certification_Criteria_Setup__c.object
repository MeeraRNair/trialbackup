    <validationRules>
        <fullName>ApprovedBusinessPlan_Validation</fullName>
        <active>true</active>
        <description>If Competency Criteria Name has  Approved Business Plan then the Compentency Critieria Value can only be “Y” or “N”</description>
        <errorConditionFormula>IF(AND( ISPICKVAL( Certification_Criteria_Name__c , &quot;Approved Business Plan&quot;) ,
  NOT(OR(Certification_Criteria_Value__c
  = &quot;Y&quot;, Certification_Criteria_Value__c
  =&quot;N&quot;))) ,true,false)</errorConditionFormula>
        <errorDisplayField>Certification_Criteria_Value__c</errorDisplayField>
        <errorMessage>Please enter Y for Yes and N for No</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CertificationCriteria_Condition_Valid</fullName>
        <active>true</active>
        <description>If Certification Criteria Name has First Sales Training / Second Sales Training / First Technical Training / Second Technical Training then   Condition should not be Null</description>
        <errorConditionFormula>IF(AND( OR(ISPICKVAL(Certification_Criteria_Name__c , &quot;First Sales Training&quot;),
  ISPICKVAL(Certification_Criteria_Name__c ,&quot;Second Sales Training&quot;),
  ISPICKVAL(Certification_Criteria_Name__c ,  &quot;First Technical Training&quot;) ,
  ISPICKVAL(Certification_Criteria_Name__c ,&quot;Second Technical Training&quot;))
  ,   ISPICKVAL( Condition__c , &apos;&apos;)  ), true, false)</errorConditionFormula>
        <errorDisplayField>Condition__c</errorDisplayField>
        <errorMessage>The Field, &quot;Condition&quot; should not be Null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CertificationCriteria_CriteriaName_Valid</fullName>
        <active>true</active>
        <description>Competency Criteria Name should not be null</description>
        <errorConditionFormula>ISBLANK(TEXT( Certification_Criteria_Name__c ))</errorConditionFormula>
        <errorDisplayField>Certification_Criteria_Name__c</errorDisplayField>
        <errorMessage>Select a Competency Criteria</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractualAgreement_Validation</fullName>
        <active>true</active>
        <description>If Competency Criteria Name has  Contractual Agreement then the Competency Criteria Value can only be “Y” or “N”</description>
        <errorConditionFormula>IF(AND( ISPICKVAL( Certification_Criteria_Name__c , &quot;Contractual Agreement&quot;), NOT(OR(Certification_Criteria_Value__c
  = &quot;Y&quot;, Certification_Criteria_Value__c
  =&quot;N&quot;))) ,true,false)</errorConditionFormula>
        <errorDisplayField>Certification_Criteria_Value__c</errorDisplayField>
        <errorMessage>Please enter Y for Yes and N for No</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DemoUnit_validation</fullName>
        <active>false</active>
        <description>If Competency Criteria Name has  Demo Unit then the Competency Critieria Value can only be EQL Demo or KACE Demo or Compellent Demo</description>
        <errorConditionFormula>IF(AND( ISPICKVAL( Certification_Criteria_Name__c , &quot;Demo Unit&quot;) ,
  NOT(OR(Certification_Criteria_Value__c
  = &quot;EQL Demo&quot;, Certification_Criteria_Value__c
  =&quot;KACE Demo&quot;,Certification_Criteria_Value__c
  = &quot;Compellent Demo&quot;, Certification_Criteria_Value__c  = &quot;Networking Demo&quot;))) ,true,false)</errorConditionFormula>
        <errorDisplayField>Certification_Criteria_Value__c</errorDisplayField>
        <errorMessage>Please enter a valid Demo Unit Type EQL Demo or KACE Demo or Compellent Demo or Networking Demo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NumberDemoUnit_Validation</fullName>
        <active>true</active>
        <description>Number of Demo Units can only be a positive numeric value.</description>
        <errorConditionFormula>IF(AND(ISPICKVAL(Certification_Criteria_Name__c , &quot;Demo Unit&quot;),OR(Number_of_Demo_Units__c &lt;= 0,ISBLANK(Number_of_Demo_Units__c))), true,false)</errorConditionFormula>
        <errorDisplayField>Number_of_Demo_Units__c</errorDisplayField>
        <errorMessage>Please enter a valid postive Number.</errorMessage>
    </validationRules>
