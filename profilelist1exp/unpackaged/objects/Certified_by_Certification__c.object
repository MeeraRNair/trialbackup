&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Onboarding - LA Spanish&quot;),&quot; Onboarding - LA Spanish; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Preferred Partner EM EMEA&quot;),&quot; Preferred Partner EM EMEA; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Premier Partner EM EMEA&quot;),&quot; Premier Partner EM EMEA; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Security&quot;),&quot; Security; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Server&quot;),&quot; Server; &quot;,&quot;&quot;)
