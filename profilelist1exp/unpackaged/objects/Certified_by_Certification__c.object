&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Information Management&quot;),&quot;Information Management; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Data Protection&quot;),&quot;Data Protection; &quot;,&quot;&quot;)</formula>
