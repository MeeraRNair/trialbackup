&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;General On Boarding EM EMEA&quot;),&quot; General On Boarding EM EMEA; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Networking&quot;),&quot; Networking; &quot;,&quot;&quot;)
&amp;
IF(INCLUDES(Account__r.Acquisition_Certification__c , &quot;Onboarding - LA English&quot;),&quot; Onboarding - LA English; &quot;,&quot;&quot;)
&amp;
