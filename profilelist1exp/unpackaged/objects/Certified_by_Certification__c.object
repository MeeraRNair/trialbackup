    <validationRules>
        <fullName>Validate_Account_Is_Changed</fullName>
        <active>true</active>
        <description>Validate if there is any changes to Account field.</description>
        <errorConditionFormula>OR( 
AND(Manually_Added_Competency__c = true, 
ISNEW(), ISBLANK(PRIORVALUE(Account__c)) 
), 
AND(Manually_Added_Competency__c = true, 
ISCHANGED(Account__c) 
) 
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Account name should not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Acquistion_Source_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( Manually_Added_Competency__c = true,
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;Compellent&apos;)),
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;SonicWALL&apos;)),
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;Wyse&apos;)),
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;Quest&apos;)),
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;Force 10&apos;)),
OR(LOWER(Acquisition_Source__c) &lt;&gt; LOWER(&apos;Sub-4&apos;))
)</errorConditionFormula>
        <errorDisplayField>Acquisition_Source__c</errorDisplayField>
        <errorMessage>The value entered is not accepted. Please refer to the helptext for a list of accepted values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Partner_Relationship_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( Manually_Added_Competency__c = true,
OR(LOWER(Partner_Relationship__c) &lt;&gt; LOWER(&apos;Registered&apos;)),
OR(LOWER(Partner_Relationship__c) &lt;&gt; LOWER(&apos;Preferred&apos;)),
OR(LOWER(Partner_Relationship__c) &lt;&gt; LOWER(&apos;Premier&apos;))
)</errorConditionFormula>
        <errorDisplayField>Partner_Relationship__c</errorDisplayField>
        <errorMessage>The value entered is not accepted. Please refer to the helptext for a list of accepted values.</errorMessage>
    </validationRules>
