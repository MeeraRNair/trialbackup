        <fullName>ValidateClone</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCLONE(), Product__c == &apos;&apos;)</errorConditionFormula>
        <errorMessage>Can not clone Custom Product with Criteria.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateEmptyRecord</fullName>
        <active>true</active>
        <description>A check to select either product /  Criteria on custom product record.</description>
        <errorConditionFormula>AND(!ISCLONE(), Product__c = &apos;&apos;, Filter_Type__c = &apos;&apos;, Criteria__c = &apos;&apos;)</errorConditionFormula>
        <errorMessage>Select either Product /  Filter Type and Criteria.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateIncludeExclude</fullName>
        <active>true</active>
        <description>To check a product with both exclude and include options.</description>
        <errorConditionFormula>AND(Include__c = true , Exclude__c = true)</errorConditionFormula>
        <errorMessage>Select either Include / Exclude</errorMessage>
    </validationRules>
