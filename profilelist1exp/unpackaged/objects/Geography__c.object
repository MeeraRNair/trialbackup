    <validationRules>
        <fullName>Country_Required_for_State_Record_Type</fullName>
        <active>true</active>
        <description>This rule is to mandate users to provide value for Country field when they select the State Record Type.</description>
        <errorConditionFormula>RecordType.Name == &apos;State&apos; &amp;&amp; ISBLANK(Country__c)</errorConditionFormula>
        <errorDisplayField>Country__c</errorDisplayField>
        <errorMessage>Country should be selected when the Geography Type is State.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Geography_Name_should_be_unique</fullName>
        <active>true</active>
        <description>The Validation Rule is to avoid duplicate values on Geography Name.</description>
        <errorConditionFormula>IF(ISNEW(), IF( UPPER(Name) == UPPER(VLOOKUP($ObjectType.Geography__c.Fields.Name,$ObjectType.Geography__c.Fields.Name, Name)),true , false), 
AND(ISCHANGED(Name), IF( UPPER(Name) == UPPER(VLOOKUP($ObjectType.Geography__c.Fields.Name,$ObjectType.Geography__c.Fields.Name, Name)),true,false)))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>This name already exists. Geography Name should be unique.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>State_and_Country_mandatory_for_ZipcodeR</fullName>
        <active>true</active>
        <description>This rule is to make State and Country mandatory and check if they are appropriate in the Zipcode Record Type</description>
        <errorConditionFormula>RecordType.Name == &apos;Zipcode&apos;  &amp;&amp; ( ISBLANK(State__c )  ||   ISBLANK( Country__c )  ||  Country__c  &lt;&gt;  State__r.Country__c )</errorConditionFormula>
        <errorMessage>State and Country are required and Country should be appropriate to the State.</errorMessage>
    </validationRules>
