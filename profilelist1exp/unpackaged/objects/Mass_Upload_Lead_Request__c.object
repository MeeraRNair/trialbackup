    <validationRules>
        <fullName>Channel_Campaign_Validation</fullName>
        <active>true</active>
        <description>Only Channel campaign can be selected when creating or editing MLUR</description>
        <errorConditionFormula>OR(
	AND(
		Campaign__r.RecordType.Name &lt;&gt; &apos;Channel Campaign&apos;,
		Campaign__r.RecordType.Name &lt;&gt; &apos;Campaign 2.0&apos;
	),
	CONTAINS(&apos;DSG Partner&apos;,  Campaign__r.Name)
)</errorConditionFormula>
        <errorMessage>Selected campaign is incorrect, or cannot be found, please contact your channel marketing team for assistance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Channel_Partner_User_Validation</fullName>
        <active>true</active>
        <description>To validate whether the Partner user selected corresponds to the Partner Account assigned to the Lead.</description>
        <errorConditionFormula>OR(
AND( 
NOT(ISBLANK(Partner_User__c)), 
NOT(ISBLANK(Partner_Account__c)), 
NOT(CONTAINS(Partner_User__r.Contact.EligibleTracks__c,Partner_Account__r.Partner_Track__r.Id)) 
),
AND(
NOT(ISBLANK(Partner_Account__c)),
ISBLANK(Partner_Account__r.Partner_Track__c))
)</errorConditionFormula>
        <errorMessage>This Partner user does not belong to the Partner Account selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MLUR_Partner_Account_Validation</fullName>
        <active>true</active>
        <description>Only Partner Account can be selected when creating or editing Channel Lead</description>
        <errorConditionFormula>AND(Partner_Account__r.RecordTypeId &lt;&gt; &apos;012A0000000TR6Y&apos;, Partner_Account__c &lt;&gt; NULL)</errorConditionFormula>
        <errorDisplayField>Partner_Account__c</errorDisplayField>
        <errorMessage>Selected account is incorrect, or cannot be found, please contact your channel marketing team for assistance.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
