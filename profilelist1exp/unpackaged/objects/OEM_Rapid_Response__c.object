    <validationRules>
        <fullName>Error_Reject_Message</fullName>
        <active>false</active>
        <errorConditionFormula>Request_Rejected__c == True &amp;&amp; ISPICKVAL(OEM_Status__c, &quot;Fixed&quot;)</errorConditionFormula>
        <errorMessage>This request cannot be rejected as this has already been fixed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Status</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT (ISPICKVAL(OEM_Status__c, &quot;New&quot;))</errorConditionFormula>
        <errorDisplayField>OEM_Status__c</errorDisplayField>
        <errorMessage>New OEM Rapid Response record shall always be in &quot;New&quot; status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OEM_Closed_status</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( OEM_Status__c, &quot;Closed&quot;) &amp;&amp;  Request_Rejected__c == False &amp;&amp; CreatedBy.Username &lt;&gt; $User.Username &amp;&amp; NOT($Permission.OEM_App_visibility)</errorConditionFormula>
        <errorMessage>Only Requestor can Close the Record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OEM_Status_Control3</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(OEM_Status__c),&quot;In Progress&quot;)&amp;&amp; ISPICKVAL(OEM_Status__c, &quot;Closed&quot;)</errorConditionFormula>
        <errorMessage>OEM Record cannot be closed when status is &quot;In Progress&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OEM_status_control</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(OEM_Status__c),&quot;Closed&quot;) &amp;&amp; ISPICKVAL(OEM_Status__c, &quot;In Progress&quot;)</errorConditionFormula>
        <errorMessage>Once Closed, OEM status cannot be &quot;In Progress&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OEM_status_control2</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(PRIORVALUE(OEM_Status__c),&quot;Closed&quot;),ISPICKVAL(PRIORVALUE(OEM_Status__c),&quot;In Progress&quot;)) &amp;&amp; ISPICKVAL(OEM_Status__c, &quot;New&quot;)</errorConditionFormula>
        <errorMessage>OEM status cannot revert back to &quot;New&quot; once Request is &quot;In Progress&quot; OR &quot;Closed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.Title==&quot;OEM_SR&quot; || $User.Title==&quot;OEM_AE&quot; ,ISPICKVAL(OEM_Status__c, &quot;In Progress&quot;))</errorConditionFormula>
        <errorMessage>The sales team can only change the status to &quot;New&quot; or &quot;Closed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RR_Reject</fullName>
        <active>true</active>
        <errorConditionFormula>Request_Rejected__c == True &amp;&amp; ISBLANK( Additional_Info_for_Rejected_Requests__c )</errorConditionFormula>
        <errorMessage>Additional information is mandatory if request is rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reject_status_control</fullName>
        <active>true</active>
        <errorConditionFormula>Request_Rejected__c == False &amp;&amp; PRIORVALUE(Request_Rejected__c)</errorConditionFormula>
        <errorMessage>You cannot Open a Rejected Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Status</fullName>
        <active>false</active>
        <errorConditionFormula>AND($User.Title==&quot;OEM_PM&quot;,Request_Rejected__c == False, OR(ISPICKVAL(OEM_Status__c, &quot;New&quot;),ISPICKVAL(OEM_Status__c, &quot;Closed&quot;)))</errorConditionFormula>
        <errorMessage>Rapid Response team can only change the status to &quot;In Progress&quot;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
