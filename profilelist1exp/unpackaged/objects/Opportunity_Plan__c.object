    <fields>
        <fullName>Competitive_Completed__c</fullName>
        <externalId>false</externalId>
        <formula>(IF(ISBLANK( Strength_1__c ), 0, 1) +
IF(ISBLANK( Strength_2__c ), 0, 1) +
IF(ISBLANK( Weakness_1__c ), 0, 1) +
IF(ISBLANK( Weakness_2__c ), 0, 1) +

IF(ISBLANK( Competitor_A_Strength_1__c ), 0, 1) +
IF(ISBLANK( Competitor_A_Strength_2__c), 0, 1) +
IF(ISBLANK( Competitor_A_Weakness_1__c ), 0, 1) +
IF(ISBLANK( Competitor_A_Weakness_2__c ), 0, 1) +

IF(ISBLANK( Competitor_B_Strength_1__c ), 0, 1) +
IF(ISBLANK( Competitor_B_Strength_2__c), 0, 1) +
IF(ISBLANK( Competitor_B_Weakness_1__c ), 0, 1) +
IF(ISBLANK( Competitor_B_Weakness_2__c ), 0, 1) +

IF(ISBLANK( Primary_Competitor_A__c ), 0, 1) +
IF(ISBLANK( Primary_Competitor_B__c ), 0, 1) +
IF(ISBLANK( Our_Position_Approach__c ), 0, 1))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Competitive Completed</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
