    <validationRules>
        <fullName>Quantity_Approved_and_Claimed</fullName>
        <active>true</active>
        <errorConditionFormula>Quantity_Claimed__c  &gt;  Quantity_Approved__c</errorConditionFormula>
        <errorMessage>Quantity Claimed should be less than or equal to Quantity Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Price_Approved</fullName>
        <active>false</active>
        <errorConditionFormula>Price_Requested__c &lt;  Approved_Price_per_unit__c</errorConditionFormula>
        <errorMessage>Price Approved should not be greater than Price Requested</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Quatity_Approved</fullName>
        <active>false</active>
        <errorConditionFormula>Quantity__c &lt; Quantity_Approved__c</errorConditionFormula>
        <errorMessage>Quantity Approved should not be greater than Quantity Requested</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_Date_not_less_than_TODAY</fullName>
        <active>false</active>
        <errorConditionFormula>OR( AND( NOT(ISBLANK(Start_Date__c)), Start_Date__c &lt; TODAY(), ISNEW() ), AND(ISCHANGED(Start_Date__c), NOT(ISBLANK(Start_Date__c)), Start_Date__c &lt; TODAY() ) )</errorConditionFormula>
        <errorMessage>Effective date should not be less than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_and_End_date_is_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK( End_Date__c )), ISBLANK(Start_Date__c)  )</errorConditionFormula>
        <errorMessage>Effective Date is required to enter Expiration Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Start_date_and_End_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK( End_Date__c )), Start_Date__c &gt;= End_Date__c  )</errorConditionFormula>
        <errorMessage>Expiration date should be greater than Effective date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_on_Quantity_Approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(Alliance_Rebate_Request__r.Status__c, &apos;Submitted&apos;),  NOT( ISBLANK( Quantity_Claimed__c ) ),  ISBLANK( Quantity_Approved__c )  )</errorConditionFormula>
        <errorMessage>Quantity Approved should not be blank when you enter Quantity Claimed</errorMessage>
    </validationRules>
