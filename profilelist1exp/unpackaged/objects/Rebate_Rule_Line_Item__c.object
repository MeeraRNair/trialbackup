    <validationRules>
        <fullName>VR_only_Rebate_or_amount_allowed</fullName>
        <active>true</active>
        <errorConditionFormula>($RecordType.Id = &apos;012A0000000VhN6&apos; || $RecordType.Id = &apos;012A0000000VhN8&apos;) &amp;&amp; ( NOT(ISNULL( Amount__c )) &amp;&amp; NOT(ISNULL( Rebate__c )) )</errorConditionFormula>
        <errorMessage>Enter either Rebate % or Amount only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Viper_Rebate_rule_Item_date</fullName>
        <active>true</active>
        <description>Effective date from / to difference should be at least 24hrs.</description>
        <errorConditionFormula>Effective_Date_To__c  -  Effective_Date_From__c  &lt;= 0</errorConditionFormula>
        <errorDisplayField>Effective_Date_To__c</errorDisplayField>
        <errorMessage>Effective To date should be at least one day after Effective From date</errorMessage>
    </validationRules>
