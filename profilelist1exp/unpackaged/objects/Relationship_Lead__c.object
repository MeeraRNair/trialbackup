    <validationRules>
        <fullName>CMAP_Cant_Associate_RL_to_Channel_Camp</fullName>
        <active>true</active>
        <description>R14.2 CMAP Validation Rule to prevent user from creating a new Relationship Lead when RT is Channel Campaign.</description>
        <errorConditionFormula>Campaign__r.RecordTypeId = &quot;012A00000012Xt5&quot;</errorConditionFormula>
        <errorMessage>Cannot associate a Relationship Lead to a Channel Campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CR8969_Route_to_Channel_Manual_Update</fullName>
        <active>true</active>
        <description>&quot;Routed to channel&quot; status option may NOT be manually chosen by a user in any of the following 3 fields. 1) Relationship lead status, 2) Relationship lead accept/ reject/ reassign status, and 3) Lead Status</description>
        <errorConditionFormula>AND((ISBLANK( Channel_Lead__c )) ,(ISPICKVAL( Status__c , &quot;Routed to Channel&quot;) || ISPICKVAL( Accept_Reject_Status__c ,&quot;Routed to Channel&quot;)))</errorConditionFormula>
        <errorMessage>Status can be Routed to Channel only if Lead is converted to a Channel Lead through routing . Click on Route to Channel button for routing Lead to Channel Lead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_Member_ID_or_Lead_ID_Req</fullName>
        <active>true</active>
        <description>The Relationship Lead needs to have a corresponding Campaign Member or Lead record, whose ID must be populated.</description>
        <errorConditionFormula>AND(ISBLANK( CM_Source__c ) ,ISBLANK( Lead_ID_Do_Not_Edit__c ))</errorConditionFormula>
        <errorDisplayField>font_color_fff_Message1__c</errorDisplayField>
        <errorMessage>A Relationship Lead record should be only created from the &quot;Convert To Relationship Lead&quot; button on a Campaign Member / Lead record, or through Dataload with the CM Source ID or Lead ID populated in the template.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_Name_Mandatory_for_Rel_Lead</fullName>
        <active>true</active>
        <description>Field is mandatory even when it is coming from integration and data load.</description>
        <errorConditionFormula>AND(
NOT($Setup.System_Behavior_Bypasser__c.Bypass_Opportunity_VRs__c),
ISBLANK( Campaign__c ) 
)</errorConditionFormula>
        <errorMessage>Campaign is mandatory for Relationship Lead records</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_name_should_not_be_changed</fullName>
        <active>true</active>
        <description>After creating a relationship lead records, the campaign name should not be changed</description>
        <errorConditionFormula>AND( NOT( ISNEW()), ISCHANGED( Campaign__c ))</errorConditionFormula>
        <errorMessage>Campaign name should not be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Channel_Consent_to_Engage_Validation</fullName>
        <active>true</active>
        <description>To check if the Consent to Engage Dell Channel Partner field is not edited by any user other than RL owner.</description>
        <errorConditionFormula>ISCHANGED(Consent_to_engage_Dell_Channel_Partner__c ) &amp;&amp; $User.Id != Relationship_Lead_Owner__r.Id &amp;&amp;  !(Channel_Lead__c !=null &amp;&amp;   ISPICKVAL(Status__c, &apos;Requires Follow-Up&apos;))</errorConditionFormula>
        <errorMessage>Customer consents to engage Partner is only editable to Relationship Lead Owner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Follow_Up_Date_is_required</fullName>
        <active>true</active>
        <description>If Unqualified Lead Reason is Future Follow-Up, Follow-Up Date should not be blank</description>
        <errorConditionFormula>AND( OR(ISPICKVAL( Unqualified_Lead_Reasons__c , &apos;Future Follow-Up&apos;), ISPICKVAL( Called_Customer__c , &quot;No&quot;) ), ISNULL(Follow_Up_Date__c) )</errorConditionFormula>
        <errorDisplayField>Follow_Up_Date__c</errorDisplayField>
        <errorMessage>Please enter the Follow-Up Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RL_Cancelled_only_by_GBL_Data_Admin</fullName>
        <active>false</active>
        <description>&quot;Cancelled - Sales inactivity&quot; should only be selectable only by profile GBL Marketing/Data Admin Profile or via DLR or by Rxx.xx GBL Admin Marketing User Non-SSO profile.</description>
        <errorConditionFormula>AND( ISPICKVAL( Status__c , &apos;Cancelled - Sales Inactivity&apos;),
NOT ( OR ( 
CONTAINS($User.Profile_Name__c, &apos;GBL Marketing/Data Admin&apos; ),
CONTAINS($User.Profile_Name__c, &apos;IT GBL Data Load&apos; ),
CONTAINS($User.Profile_Name__c, &apos;GBL Admin Marketing User - Non SSO&apos; )
) ))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only marketing admin users may apply the ‘Cancelled – Sales Inactivity’ Relationship Lead status to leads.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RL_and_Inactive_Campaign_Rule</fullName>
        <active>true</active>
        <description>Relationship Lead records should not be created for Inactive Campaigns</description>
        <errorConditionFormula>AND(ISNEW(), NOT( Campaign__r.IsActive = TRUE ))</errorConditionFormula>
        <errorMessage>Relationship Lead records should not be created for Inactive Campaigns</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RL_be_default_Req_FollowUp_status</fullName>
        <active>true</active>
        <description>Relationship Lead status on creation should be Requires Follow-Up</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Status__c,&quot;Requires Follow-Up&quot;)),ISNEW())</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Relationship Lead record must have  &quot;Requires Follow-Up&quot; status on creation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RL_record_shouldnt_exist_for_Sales_Touch</fullName>
        <active>true</active>
        <description>Relationship Lead records should not be created for Account campaigns</description>
        <errorConditionFormula>Campaign__r.Is_this_Account_Play_Campaign__c = TRUE</errorConditionFormula>
        <errorMessage>Relationship Lead records should not be created for Account campaigns</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rejection_Reason_is_Mandatory</fullName>
        <active>false</active>
        <description>Rejection Reason is Mandatory for rejected leads</description>
        <errorConditionFormula>AND(ISPICKVAL( Accept_Reject_Status__c , &quot;Rejected By Sales&quot;), ISPICKVAL( Rejection_Reason__c , &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Rejection_Reason__c</errorDisplayField>
        <errorMessage>Please select a Rejection Reason</errorMessage>
    </validationRules>
