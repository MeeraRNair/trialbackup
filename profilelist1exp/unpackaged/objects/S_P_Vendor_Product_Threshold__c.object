    <validationRules>
        <fullName>EA_Level_is_Selected_VR</fullName>
        <active>true</active>
        <description>If Level is EA, then the Agreement Level, Percentage, Threshold Required fields must be specified.</description>
        <errorConditionFormula>AND(   ISPICKVAL( Level__c , &quot;EA&quot;) ,  OR(ISBLANK(Agreement_Level__c  ),  ISBLANK(Percentage__c )  ,  Threshold_Required__c = false))</errorConditionFormula>
        <errorMessage>If Level is EA, then the Agreement Level, Percentage, Threshold Required fields must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Level_is_Empty_VR</fullName>
        <active>true</active>
        <description>Level field must be specified.</description>
        <errorConditionFormula>ISPICKVAL(Level__c , &apos;&apos;)</errorConditionFormula>
        <errorMessage>If Level is empty, Level field must be specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Level_is_Selected_VR</fullName>
        <active>true</active>
        <description>If Level is Product, then the Product Family, Product Description,  Percentage,  Minimum Revenue fields must be specified.</description>
        <errorConditionFormula>AND(   ISPICKVAL( Level__c , &quot;Product&quot;) ,  OR(ISBLANK(Product_Family__c ),  ISBLANK(Product_Description__c )  , ISBLANK( Percentage__c ) ,  ISBLANK(Minimum_Revenue__c )))</errorConditionFormula>
        <errorMessage>If Level is Product, then the Product Family, Product Description,  Percentage,  Minimum Revenue fields must be specified.</errorMessage>
    </validationRules>
