    <validationRules>
        <fullName>Group_is_required</fullName>
        <active>true</active>
        <description>This rule is to make the Group field required when created through API</description>
        <errorConditionFormula>ISPICKVAL(Group__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Group__c</errorDisplayField>
        <errorMessage>Group is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Role_Territory_or_Group_cannot_be_change</fullName>
        <active>true</active>
        <description>Role, Territory and Group in the ‘Territory Sales User Assignment’ can not be changed once the record is saved</description>
        <errorConditionFormula>ISCHANGED( Role__c ) || ISCHANGED( Territory__c ) || ISCHANGED( Group__c)</errorConditionFormula>
        <errorMessage>Role, Territory or Group cannot be changed once the record is saved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_cannot_be_changed_for_Inactive_reco</fullName>
        <active>true</active>
        <description>This rule is avoid changing the User when the record is Inactive.</description>
        <errorConditionFormula>(Is_Active__c = FALSE || PRIORVALUE(Is_Active__c) = FALSE ) &amp;&amp;  ISCHANGED( User__c)</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>User cannot be changed when the record is Inactive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_is_required</fullName>
        <active>true</active>
        <description>This rule is to make the User field required when created through API</description>
        <errorConditionFormula>ISBLANK(User__c )</errorConditionFormula>
        <errorDisplayField>User__c</errorDisplayField>
        <errorMessage>User is required!!</errorMessage>
    </validationRules>
