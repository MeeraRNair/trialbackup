    <validationRules>
        <fullName>User_Validation</fullName>
        <active>true</active>
        <description>This field should allow only Internal Users</description>
        <errorConditionFormula>CONTAINS(User__r.Profile_Name__c, &quot;Portal&quot;)</errorConditionFormula>
        <errorMessage>Partner Users and Customer Portal Users are not allowed.  Please choose a User.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Viper_Email_Subscription_Notification</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Email__c  &lt;&gt;  $User.Email ,NOT(Contains( $Profile.Name,&apos;Marketing&apos;)))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Users are only allowed to create/ update their own email subscription.</errorMessage>
    </validationRules>
